  "profiles": [
    {
      "complex_modifications": {
        "parameters": {
          "basic.simultaneous_threshold_milliseconds": 50,
          "basic.to_delayed_action_delay_milliseconds": 500,
          "basic.to_if_alone_timeout_milliseconds": 1000,
          "basic.to_if_held_down_threshold_milliseconds": 500,
          "mouse_motion_to_scroll.speed": 100
        },
        "rules": [
          {
            "manipulators": [
              {
                "description": "Change Caps lock to ESC",
                "from": {
                  "key_code": "caps_lock"
                },
                "to": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "manipulators": [
              {
                "description": "Change ESC to command+ctrl+alt (for Mac keyboard)",
                "from": {
                  "key_code": "escape",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_command",
                    "modifiers": ["left_control", "left_option"]
                  }
                ],
                "type": "basic"
              }
            ]
          },

          {
            "manipulators": [
              {
                "description": "Change end to command+ctrl+alt (for kinesis keyboard)",
                "from": {
                  "key_code": "end",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_command",
                    "modifiers": ["left_control", "left_option"]
                  }
                ],
                "type": "basic"
              }
            ]
          },

          {
            "manipulators": [
              {
                "description": "Left and Right Shift together -> Caps Lock",
                "from": {
                  "key_code": "right_shift",
                  "modifiers": {
                    "mandatory": ["left_shift"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "caps_lock"
                  }
                ],
                "type": "basic"
              }
            ]
          },

          {
            "manipulators": [
              {
                "description": "Right and Left Shift together -> Caps Lock",
                "from": {
                  "key_code": "left_shift",
                  "modifiers": {
                    "mandatory": ["right_shift"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "caps_lock"
                  }
                ],
                "type": "basic"
              }
            ]
          }
        ]
